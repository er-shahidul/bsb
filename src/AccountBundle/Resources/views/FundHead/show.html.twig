{% extends 'base.html.twig' %}

{% block body %}
    <h1>Fundhead</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fundHead.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ fundHead.name }}</td>
            </tr>
            <tr>
                <th>Sort</th>
                <td>{{ fundHead.sort }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('fundhead_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('fundhead_edit', { 'id': fundHead.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
